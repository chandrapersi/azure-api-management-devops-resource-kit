{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "apimServiceName": {
      "type": "string"
    },
    "loggerResourceId": {
      "type": "object"
    }
  },
  "resources": [
    {
      "apiVersion": "2021-08-01",
      "type": "Microsoft.ApiManagement/service/loggers",
      "name": "[concat(parameters('apimServiceName'), '/appi-psl-dev-g7iea-logs')]",
      "properties": {
        "loggerType": "applicationInsights",
        "credentials": {
          "instrumentationKey": "{{Logger-Credentials--626931d04634610edcbffcf4}}"
        },
        "isBuffered": true,
        "resourceId": "[parameters('loggerResourceId').appipsldevg7iealogs]"
      }
    },
    {
      "apiVersion": "2021-08-01",
      "type": "Microsoft.ApiManagement/service/loggers",
      "name": "[concat(parameters('apimServiceName'), '/azuremonitor')]",
      "properties": {
        "loggerType": "azureMonitor",
        "isBuffered": true,
        "resourceId": "[parameters('loggerResourceId').azuremonitor]"
      }
    }
  ]
}